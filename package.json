{
  "name": "jigsaw",
  "version": "0.0.0",
  "description": "An app of jigsaw puzzles",
  "main": "index.js",
  "scripts": {
    "clean": "recursive-delete 'dist'",
    "js": "rollup --config",
    "css": "node sass.js",
    "copy": "recursive-copy 'src/copy' 'dist'",
    "img": "imagemin src/img/* --out-dir=dist/img --plugin=pngquant --plugin=mozjpeg --plugin=pngcrush --plugin=zopfli",
    "build-dirty": "npm-run-all -p js css copy img",
    "build": "npm-run-all -s clean build-dirty",
    "watch-css": "chokidar './src/**/*.scss' -c 'npm run css'",
    "watch-js": "chokidar './src/**/*.js' -c 'npm run js'",
    "watch-copy": "chokidar './src/copy/**/*.*' -c 'npm run copy'",
    "watch-img": "chokidar './src/img/**/*.*' -c 'npm run img'",
    "watch": "npm-run-all -p build watch-css watch-js watch-copy watch-img",
    "server-start": "browser-sync start --files 'dist' --server 'dist'",
    "start": "npm-run-all -p watch server-start"
  },
  "repository": {
    "url": ""
  },
  "author": "Juan Bernardo Benavides",
  "license": "MIT",
  "dependencies": {
    "@interactjs/actions": "^1.10.11",
    "@interactjs/auto-start": "^1.10.11",
    "@interactjs/dev-tools": "^1.10.11",
    "@interactjs/interact": "^1.10.11",
    "reefjs": "^10.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.6",
    "browser-sync": "^2.27.5",
    "chokidar-cli": "^3.0.0",
    "imagemin-cli": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngcrush": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-zopfli": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "recursive-fs": "^2.1.0",
    "rollup": "^2.58.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.43.3"
  }
}
